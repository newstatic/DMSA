<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- 禁用沙盒以允许文件系统操作 -->
    <key>com.apple.security.app-sandbox</key>
    <false/>

    <!-- 允许继承的 Mach 服务 -->
    <key>com.apple.security.inherit</key>
    <true/>
</dict>
</plist>
